{
  "name": "Bear Food Extension",
  "version": "1.0",
  "description": "Chrome extension for real-time dining halls information.",
  "manifest_version": 3,
  "icons": {
    "16": "images/square-logo.png",
    "48": "images/square-logo.png",
    "128": "images/square-logo.png"
  },
  "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/square-logo.png",
        "48": "images/square-logo.png",
        "128": "images/square-logo.png"
      }
  },
  "permissions": [
    "activeTab"
  ]
}
